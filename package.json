{
  "name": "next-js-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:dev": "prettier --write src/",
    "format:prod": "prettier --write .next",
    "check-format:dev": "prettier --check src/",
    "check-format:prod": "prettier --check .next",
    "prepare": "husky install",
    "cypress": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.1.0",
    "@mapbox/search-js-react": "^1.0.0-beta.16",
    "@next/font": "13.1.6",
    "@supabase/auth-helpers-nextjs": "^0.6.1",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-ui-react": "^0.3.5",
    "@supabase/supabase-js": "^2.21.0",
    "@types/moment": "^2.13.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "bootstrap": "^5.2.3",
    "bootswatch": "^5.2.3",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "moment": "^2.29.4",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-csv": "^2.2.2",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "typescript": "4.9.5",
    "yup": "^1.1.1",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/react-csv": "^1.1.3",
    "cypress": "^12.9.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "start-server-and-test": "^1.15.4",
    "supabase": "^1.50.12"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,css,md}": "prettier --write"
  }
}
